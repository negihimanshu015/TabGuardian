{
    "manifest_version": 3,
    "name": "TabGuardian",
    "description": "Recover Tab",
    "version": "1.0",
    "action": { 
        "default_popup":"TabGuardian/TabGuardian.html"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },    
    "permissions":[        
        "storage",
        "scripting",
        "tabs"

    ],
    "host_permissions":[
        "http://*/*",
        "https://*/*"
    ]   
    
}